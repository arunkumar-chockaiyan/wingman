{
  "name": "wingman",
  "version": "1.0.0",
  "description": "Real-time sales assistant orchestration with Kafka and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "infra:up": "docker-compose -f docker-compose.yml up -d",
    "infra:down": "docker-compose -f docker-compose.yml down",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "db:setup": "prisma migrate dev && ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "ArunKumar Chockaiyan",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^7.4.1",
    "@tavily/core": "^0.7.1",
    "cors": "^2.8.6",
    "cross-env": "^10.1.0",
    "dotenv": "^16.0.0",
    "express": "^5.2.1",
    "google-tts-api": "^2.0.2",
    "kafkajs": "^2.2.0",
    "socket.io": "^4.8.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.18.1",
    "prisma": "^7.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}